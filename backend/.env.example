# Port de l'application
PORT=3000

# Base de données
MONGO_URI=mongodb://localhost:27017/garage-agency

# Clé secrète pour JWT (si tu utilises des tokens pour l'authentification)
JWT_SECRET=supersecretkey

# Clé API de service externe (par exemple, un service de paiement ou un service tiers)
API_KEY=your_api_key_here

# Variables de production (exemple : variables de production, que tu ne veux pas partager)
NODE_ENV=development

# Envoie des emails avec un service tiers (par exemple, SendGrid ou SMTP)
MAIL_HOST=smtp.example.com
MAIL_PORT=587
MAIL_USER=email@example.com
MAIL_PASS=emailpassword

# Variables spécifiques à l'authentification (par exemple, un secret pour la session)
SESSION_SECRET=another_secret_key

# URL d'un service de stockage en cloud (par exemple, AWS S3 ou Google Cloud Storage)
CLOUD_STORAGE_BUCKET=my-bucket
CLOUD_STORAGE_KEY=my_cloud_key
CLOUD_STORAGE_SECRET=my_cloud_secret
